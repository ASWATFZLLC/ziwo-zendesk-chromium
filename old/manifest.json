{ "background":
  { "scripts": [ "js/libs/pegjs.min.js"
               , "js/libs/bhiv/lib/browser/bootstrap.js"
               , "js/libs/EventEmitter.min.js"
               , "js/libs/socket.io-1.3.5.js"
               , "js/core.js"
               ]
  }
, "content_scripts":
  [ { "all_frames": true
    , "js": [ "js/inject-ziwo.js" ]
    , "matches": [ "*://*.aswat.co/*" ]
    , "run_at": "document_end"
    }
  , { "all_frames": true
    , "js": [ "js/inject-zendesk.js" ]
    , "matches": [ "*://*.zendesk.com/*" ]
    , "run_at": "document_start"
    }
  ]
, "description": "Bridge between Ziwo and Zendesk"
, "icons":
  { "16": "images/ds_icon_16.png"
  , "48": "images/ds_icon_48.png"
  , "64": "images/ds_icon_64.png"
  , "128": "images/ds_icon_128.png"
  }
, "manifest_version": 4
, "name": "Ziwo Zendesk Bridge"
, "content_security_policy": "object-src 'self'; script-src 'self' 'unsafe-eval'; connect-src 'self' ws://*.aswat.co https://*.aswat.co https://*.zendesk.com"
, "web_accessible_resources": ["images/*", "js/libs/*", "js/frame-*.js"]
, "permissions": [ "tabs", "webRequest", "*://*.aswat.co/*", "*://*.zendesk.com/*" ]
, "version": "0.0.4"
}
